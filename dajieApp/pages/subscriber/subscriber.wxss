	background: #fff;
	padding: 20rpx;
}
.jobName{
	font-size: 30rpx;
	line-height: 40rpx;
	color: #333333;
}
.detail-msg{
	font-size: 26rpx;
	line-height: 30rpx;
	color: #999999;
	padding: 15rpx 0;
}
.detail-msg view{
	display: inline-block;
	/*margin-right: 15rpx;
