/* pages/resume/resume.wxss */
.yellow-tips{
	background: #FFFCDA;
	height: 90rpx;
	line-height: 90rpx;
	width: 100%;
}
.yellow-tips view{
	font-size: 30rpx;
	color: #F96100;
	width: 100%;
	text-align: center;
}
.detail-box{
	padding: 110rpx 0 60rpx;
	float: left;
}
.detail-left{
	margin-left: 110rpx;
	float: left;
}
.detail-left image{
	width: 40rpx;
	height: 40rpx;
}
.detail-left view{
	height: 200rpx;
	margin: 5rpx 18.5rpx 10rpx;
 	border-left:1rpx dashed #A3A3A3;
}
.detail-right{
	float: left;
	width: 450rpx;
	font-size: 28rpx;
	color: #353535;
	margin-left:20rpx;
}
.text-first view{
	float: left;
}
.blue-text{
	display: inline-block;
	color: #00B6D7;
}
.detail-right image{
	width: 380rpx;
	height: 100rpx;
	margin: 45rpx 0 75rpx 0;
}
.code-btn{
	background: #00B6D7;
	border-radius: 4px;
	font-size: 32rpx;
	color: #FFFFFF;
	text-align: center;
	width: 440rpx;
	height: 80rpx;
	line-height: 80rpx;
	float: left;
	margin-left: 150rpx;
	display: flex;
	align-items:center;
	justify-content:center;
}
.code-btn image{
	width: 32rpx;
	height: 32rpx;
	margin-right: 3%;
}
.code-btn view{
	display: inline-block;
	width: auto;
	font-size: 32rpx;
	color: #FFFFFF;
}
.mine-msg{
	width: 100%;
	padding: 3% 0;
	background: #fff;
}
.all-list{
	line-height: 40rpx;
	padding: 10rpx 30rpx;
	display: -weikit-box;
	height: auto;
}
.all-list .width-half{
	width: 50%;
	display: inline-block;
}
.left-tit{
	font-size: 30rpx;
	color: #999999;
	width: 160rpx;
	display: inline-block;
}
.right-msg{
	font-size: 30rpx;
	color: #151515;
	-webkit-box-flex:1;
	display: inline-block;
}
.all-list.ads{
	background: #fff;
	display: -webkit-box;
	position: relative;
}
.all-list.ads .left-tit{
	width: 160rpx;
	position: absolute;
	left: 30rpx;
}
.all-list.ads .right-msg{
	line-height: 40rpx;
	word-wrap:break-word;
	padding-left: 160rpx;
	-webkit-box-flex:1;
}
.asp-msg,.edu-msg{
	margin-top: 20rpx;
}
.title{
	font-size: 32rpx;
	color: #333333;
	font-weight: 600;
	padding: 0 5% 3%;
	margin-bottom: 3%;
	line-height: 50rpx;
	border-bottom: 0.5rpx solid #ebebeb;
}
.all-list.ads.truncate{
	position: relative;
}
.all-list.ads.truncate .left-tit{

}
.truncate .right-msg{
	text-overflow: -o-ellipsis-lastline;
	overflow: hidden;
     text-overflow: ellipsis;
	display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    margin-bottom: 10rpx;
}
.edu{
	line-height:40rpx;
	padding:10rpx 5%;
}
.edu-time,
.edu-major,
.edu-type{
	font-size: 28rpx;
	color: #999999;
	padding: 10rpx 0;
}
.edu-school{
	font-size: 30rpx;
	color: #333;
	padding: 10rpx 0;
}
.edu-text{
	font-size: 30rpx;
	color: #333333;
	padding: 10rpx 0;
	line-height: 50rpx;
}
.edu-time .type{
	display: inline-block;
	font-size: 20rpx;
	color: #FFFFFF;
	background: #ECA79A;
	border-radius: 2px;
	padding: 0 3rpx;
	line-height: 30rpx;
	margin-left: 3%;
}
.mineText,.project-msg,.speciality,.language,.it{
	margin-top: 20rpx;
	border-top: 1px solid #ebebeb;
}
.mine-text{
	padding:10rpx 5%;
	font-size: 30rpx;
	color: #333;
}
.person-num{
	display: inline-block;
	color: #333;
}
.speciality-box{
	padding:10rpx 5%;
}
.speciality-list{
	background: #3ECAE4;
	border-radius: 2px;
	line-height: 60rpx;
	padding: 0 10rpx;
	font-size: 28rpx;
	color: #FFFFFF;
	display: inline-block;
	margin: 0 20rpx 20rpx 0;
}
.language-box{
	margin-left: 5%;
	padding-right: 5%;
	padding-top: 10rpx;
	padding-bottom: 10rpx;
	font-size: 30rpx;
	display: -webkit-box;
	color: #333333;
	width: 90%;
}
/* .language .language-box */.border{
	border-bottom: 0.5rpx solid #ebebeb;
	padding-bottom: 25rpx;
	margin-bottom: 15rpx;
}
.language-left,
.language-right{
}
.language-right{
	-webkit-box-flex:1;
}
.language-left{
	margin-right: 6%;
}
.language-right-top{
	display: -webkit-box;
	width: 100%;
	padding-bottom: 10rpx;
}
.language-right-top-half{
	-webkit-box-flex:1;
}
.half-left ,
.half-right{
	display: inline-block;
}
.half-left{
	color: #ccc;
}
.language-right-bottom{
	font-size:28rpx;
	color: #666666;
	line-height: 50rpx;
}
.it .margin{
	margin-left: 5%;
	padding-right: 5%;
}
.it .border{
	padding-top: 10rpx;
	padding-bottom: 35rpx;
}
.it-text{
	width: 100%;
	display: -webkit-box;
	font-size: 30rpx;
	color: #000000;
	line-height: 50rpx;
}
.it-text view{
	-webkit-box-flex:1;
	margin-bottom: 20rpx;
}
.it-text view.it-text-right{
	text-align: right;
	color: #ccc;
}
.train-msg,.school-msg,.other-msg,.wanted-msg,.accessory-msg{
	margin-top: 20rpx;
}
.school-text{
	color: #666;
	font-size: 28rpx;
}
.accessory-msg .edu{
	width: 100%;
	display: -webkit-box;
	align-items:center;
	justify-content:center;
}
.accessory-msg .edu .edu-time{
	width: 32rpx;
	height: 32rpx;
	margin-right: 3%;
}
.accessory-msg .edu .edu-time image{
	width: 100%;
	display: inline-block;
	height: 100%;
	align-items:center;
	justify-content:center;
}
.last{
	width:100%;
	background:#f6f7f8;
	padding-bottom:100rpx;
}
